{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": false,
        "importHelpers": true,
        "lib": [
            "ES2015",
        ]
    },
}